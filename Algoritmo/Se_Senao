Algoritmo "SE_SENAO"

Var
dinheiro: real


Inicio
escreva("Quanto voce tem pra viajar?")
leia(dinheiro)

se(dinheiro>=10000)entao
   escreval("Partiu Disney")
senao
     se(dinheiro>=5000) e (dinheiro<10000)entao
        escreva("Partiu Argentina")
     senao
          escreva("ficar em casa #Pobre!")
     fimse



Fimalgoritmo
