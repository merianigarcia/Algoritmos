Algoritmo "IMC"

Var
   m, a, imc: real


Inicio
   escreval ("digite sua massa em (kg): ")
   leia (m)
   escreval ("digite sua Altura (M): ")
   leia (a)
   imc <- m / (a * a )
   escreval ("imc: ", imc:5:2)
   se (imc >= 18.5) e (imc < 25) entao
      escreval ("Seu peso esta ideal para os parametros")
   senao
      escreva("voce nao esta dentro dos parametros")
   Fimse

Fimalgoritmo
